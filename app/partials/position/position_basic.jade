form(name="posEditForm" ng-submit="save(currentPos.$save(), posEditForm)" ng-controller="EditableResource" ng-init="params = {}")
    .form-btn-group(ng-hide="editing")
        md-button.skyblue.md-raised(ng-click="edit($event)") 编辑
    .form-btn-group(ng-show="editing")
        md-button.md-raised.white(ng-click="cancel(currentPos,$event, posEditForm)",) 取消
        md-button.skyblue.md-raised(type="submit") 保存
    fieldset(ng-disabled="!editing")
        div(layout)
            md-input-container(flex)
                label OA文件编号
                input(ng-model="currentPos.oaFileNo")
            md-input-container(flex)
                label 创建时间
                input(disabled type="date" ng-model="currentPos.createAt")
        div(layout)
            md-input-container(flex)
                label 岗位名称
                input(ng-model="currentPos.name")
            md-input-container(flex)
                label 所属部门
                input(disabled ng-model="currentPos.department.name")
                //- ui-select(
                //- ng-model="currentPos.department"
                //- ng-change="params.orgPos = currentPos.department.positions.length? currentPos.department.positions: currentPos.department.positions.$fetch();currentPos.position = null"
                //- ng-init="params.orgPos = currentPos.department.positions.length? currentPos.department.positions: currentPos.department.positions.$fetch();currentPos.position = null"
                //- reset-search-input="true"
                //- style="width: 500px"
                //- )
                //-     ui-select-match(placeholder="请输入机构名称") {{ $select.selected.name }}
                //-     ui-select-choices(repeat="org in allOrgs | filter:$select.search | limitTo: 5") {{ org.name }}
        div(layout)
            md-input-container.row-has-value(flex)
                label 分类
                md-select(ng-model="currentPos.category" md-on-open="loadEnum('categories')")
                    md-select-label {{currentPos.category? currentPos.category.displayName : '无'}}
                    md-option(ng-value="category" ng-repeat="category in enums.categories") {{category.displayName}}
            md-input-container.row-has-value(flex)
                label 通道
                md-select(ng-model="currentPos.channel" md-on-open="loadEnum('channels')")
                    md-select-label {{currentPos.channel? currentPos.channel.displayName : '无'}}
                    md-option(ng-value="channel" ng-repeat="channel in enums.channels") {{channel.displayName}}
        div(layout)
            md-input-container(flex)
                label 类别
                input(ng-model="currentPos.postType")
            md-input-container(flex)
                label 编制
                input(ng-model="currentPos.budgetedStaffing")
        div(layout)
            md-input-container(flex)
                label 在岗人数
                input(ng-model="currentPos.staffing" disabled)
            md-input-container(flex)
                label 超编
                input(ng-model="currentPos.overstaffedNum" disabled)
        div(layout)
            //- md-input-container(flex)
            //-     label 职务职级
            //-     input(ng-model="")
            md-input-container(flex)
                label 工时制度
                input(ng-model="currentPos.schedule.displayName")
            md-input-container(flex)
                label 岗位性质
                input(ng-model="currentPos.positionNature.displayName")

