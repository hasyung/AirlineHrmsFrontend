md-dialog()
    md-subheader 
        h3 {{::dialog.change.name}}{{::dialog.change.action}}"{{::dialog.change.auditableType}}"
        md-button.pull-right(ng-click="dialog.close()") 关闭
    md-content
        table.table.table-striped.custom-table.popup-table
            tr
                th() 字段
                th() 值
            tr(ng-repeat="(field, val) in dialog.change.auditedChanges", ng-if="field != 'unused'")
                td(ng-bind="field")
                td.overflow-text(ng-bind="val || '无'", ng-attr-title="{{val}}")
