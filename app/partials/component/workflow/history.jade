.app-content-wrapper(flex ng-init="ctrl.historyList()")
    div(nb-filter="ctrl.filterOptions" on-search="ctrl.search(state)")
    div.ui-grid-container
        md-toolbar
            .md-toolbar-tools
                h2 #{flowName}历史列表
                div(flex)
                block history_list_actions

        div(nb-grid column-defs="ctrl.columnDef" on-register-api="ctrl.exportGridApi(gridApi)"  grid-selection multi-select= hisMultiSelect? 'true': 'false' safe-src="ctrl.tableData")