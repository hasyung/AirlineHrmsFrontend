
tabset
    tab(heading="待审核")
        .position-manage.app-content-wrapper
            div(nb-table='hisLeaders' nb-safe-src="hisLeaders")
                input(type="hidden" nb-pipe="ctrl.search(tableState)")
                .table-container
                    h4 人员明细{{hisLeaders.length}}
                    table.table.table-striped.custom-table
                        thead
                            tr
                                //- th(select-row="" mode='all')
                                th 工号
                                th 姓名
                                th 到岗时间
                                th 离岗时间
                                
                        tbody
                            tr(ng-repeat="emp in hisLeaders track by emp.id")
                                //- td(select-row="emp" mode="multiple")
                                td(ng-bind="::emp.employee.employeeNo")
                                td(ng-bind="emp.employee.name")
                                td(ng-bind="::emp.startDate")
                                td(ng-bind="::emp.endDate")
    tab(heading="变更记录")
        include personnel_changes_record
