md-dialog(ng-init="change = data")
    md-subheader 
        h3 {{change.name}}{{change.action}}"{{change.auditableType}}"
    md-content
        table.table.table-striped.custom-table.popup-table
            tr
                th() 字段
                th() 值
            tr(ng-repeat="(field, val) in change.auditedChanges", ng-if="field != 'unused'")
                td(ng-bind="field")
                td.overflow-text(ng-bind="val || '无'", ng-attr-title="{{val}}")
