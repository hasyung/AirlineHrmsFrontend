.my-requests(flex)
    .my-requests-header 考勤 / 劳动关系 / 培训 / 绩效
    .my-cell-cotainer
        a.my-requests-cell(ui-sref=".leave")
            md-button.md-fab
                md-icon(md-svg-src="/images/svg/flow/ask_for_leave.svg")
            .desc 我要请假
        a.my-requests-cell(ui-sref=".")
            md-button.md-fab
                md-icon(md-svg-src="/images/svg/flow/turn.svg")
            .desc 我要转正
        a.my-requests-cell(ui-sref=".resignation")
            md-button.md-fab
                md-icon(md-svg-src="/images/svg/flow/resignation.svg")
            .desc 我要辞职
        a.my-requests-cell(ui-sref=".adjust_position")
            md-button.md-fab
                md-icon(md-svg-src="/images/svg/flow/adjust_position.svg")
            .desc 我要调岗
        a.my-requests-cell(ui-sref=".erarly_retirement")
            md-button.md-fab
                md-icon(md-svg-src="/images/svg/flow/early_retirement.svg")
            .desc 我要退养
        a.my-requests-cell(ui-sref=".renew_contract")
            md-button.md-fab
                md-icon(md-svg-src="/images/svg/flow/contract.svg")
            .desc 合同续签
        a.my-requests-cell(ui-sref=".")
            md-button.md-fab
                md-icon(md-svg-src="/images/svg/flow/degree.svg")
            .desc 学历变更
        a.my-requests-cell(ui-sref=".")
            md-button.md-fab
                md-icon(md-svg-src="/images/svg/flow/performance.svg")
            .desc 绩效申述结果






