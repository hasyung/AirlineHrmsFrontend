extends ./../datas_layout.jade

block vars
	- var pickers = ['福利费用', '福利费用派', '汇报']

block datas_content
	.datas__container--reports(ng-if="ctrl.datasType == '汇报'")
		include ./welfare_reports
	.datas__container--charts(ng-if="ctrl.datasType == '福利费用'")
		include ./welfare_fees
	.datas__container--charts(ng-if="ctrl.datasType == '福利费用派'")
		include ./welfare_fees_pie