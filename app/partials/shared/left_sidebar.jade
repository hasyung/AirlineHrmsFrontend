md-sidenav.nb-nav(md-is-locked-open="true")
    md-content(flex role="navigation")
        ul.nb-menu
            li.parent-list-item(ng-repeat="section in menu.sections")
                menu-link(ng-if="section.type == 'link'" page="section")
                menu-toggle(section="section" ng-if="section.type != 'link'")